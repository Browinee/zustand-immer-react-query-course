{
  "name": "react-query-course",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:5000",
  "scripts": {
    "start": "parcel serve ./src/index.html",
    "backend": "json-server --watch --delay=1000 db.json",
    "start:fullstack": "concurrently \"yarn backend\" \"yarn start\"",
    "build": "parcel build ./src/index.html --no-source-maps",
    "eject": "react-scripts eject",
    "test": "react-scripts test",
    "eslint": "eslint --ext js,ts,tsx",
    "checkTs": "tsc --noEmit",
    "prepare": "husky install"
  },
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.6.2",
    "@mui/lab": "5.0.0-alpha.78",
    "@mui/material": "^5.6.2",
    "@mui/styles": "^5.6.2",
    "@xstyled/emotion": "^3.6.0",
    "axios": "^0.26.1",
    "clsx": "^1.1.1",
    "formik": "^2.2.9",
    "immer": "^9.0.12",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-query": "^3.35.0",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "^5.0.1",
    "react-table": "^7.7.0",
    "simple-zustand-devtools": "^1.0.1",
    "typestyle": "^2.3.0",
    "web-vitals": "^1.1.2",
    "yup": "^0.32.11",
    "zustand": "4.0.0-rc.0",
    "zustand-persist": "^0.4.0"
  },
  "devDependencies": {
    "@faker-js/faker": "6.1.2",
    "@parcel/transformer-react-refresh-wrap": "^2.4.1",
    "@testing-library/dom": "^8.13.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.1.1",
    "@testing-library/react-hooks": "^8.0.0",
    "@testing-library/user-event": "^14.1.1",
    "@types/common-tags": "^1.8.1",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.25",
    "@types/react": "^18.0.5",
    "@types/react-dom": "^18.0.1",
    "@types/react-table": "^7.7.10",
    "@types/uuid": "^8.3.4",
    "@types/yup": "^0.29.13",
    "concurrently": "^7.1.0",
    "coveralls": "^3.1.1",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react-hooks": "^4.4.0",
    "husky": "^7.0.4",
    "istanbul-lib-coverage": "^3.2.0",
    "json-server": "^0.17.0",
    "lint-staged": "^12.3.8",
    "msw": "^0.39.2",
    "netlify-plugin-cypress": "^2.2.0",
    "parcel": "^2.4.1",
    "prettier": "^2.6.2",
    "react-test-renderer": "^18.0.0",
    "ts-jest": "^27.1.4",
    "typescript": "^4.6.3",
    "uuid": "^8.3.2"
  },
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "alias": {
    "process": false
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "yarn run eslint --fix"

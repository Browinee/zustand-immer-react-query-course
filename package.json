    "concurrently": "^7.3.0",
    "coveralls": "^3.1.1",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "husky": "^8.0.1",
    "istanbul-lib-coverage": "^3.2.0",
    "json-server": "^0.17.0",
    "lint-staged": "^13.0.3",
    "msw": "^0.44.2",
    "netlify-plugin-cypress": "^2.2.0",
    "parcel": "^2.7.0",
    "prettier": "^2.7.1",
    "react-test-renderer": "^18.1.0",
    "ts-jest": "^27.1.5",
    "typescript": "^4.7.3",
    "uuid": "^8.3.2"
  },
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "alias": {
    "process": false
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "yarn run eslint --fix"
